CREATE TABLE tbEmployeeMaster
(
                EmployeeId                       INT AUTO_INCREMENT PRIMARY KEY NOT NULL,
                EmployeeName                  NVARCHAR(50),
                Gender                                NVARCHAR(10),
                Department                         NVARCHAR(50)
)
;
